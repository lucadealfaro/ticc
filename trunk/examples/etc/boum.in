(* 
Action boum increases x both in A and in B.  Since A and B can also
emit boum, the only way A and B are compatible is if x is kept in
synch.  Luckily, it is. 
*)

open Ticc;;

parse "boum.si" ;;

let a = mk_sym "A";;
let b = mk_sym "B";;

(*
print_symmod a;;
print_symmod b;;
*)

let c = compose a b;;

print_symmod c;;

